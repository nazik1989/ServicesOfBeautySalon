@using ServicesOfBeautySalon.Models

@model UserModel

@{
    ViewBag.Title = "OneUser";
    ViewBag.PageHeaderText = "User Page";
}

<h6>OneUser</h6>


<h3>ID :  @Model.ID </h3>
<h2>FirstName: @Model.FirstName </h2>
<h2>LastName: @Model.LastName </h2>
<img  src=~/images/@Model.ImageURL class="img-oneService" alt=@Model.ImageURL />
<p>Mail: @Model.Mail</p>
<p>Phone:  @Model.Phone</p>
<p>UserName:  @Model.UserName</p>
<p>Password:  @Model.Password</p>

